{% from "./helpers.html.j2" import sponsors_group, toc %}
{% extends "layout.html.j2" %}

{% block content %}
  <div id="languages">
    <h1>Sponsors by native language</h1>

    {{ toc(languages.keys()) }}
    {% for language, _sponsors in languages.items() %}
    {{ sponsors_group(language, _sponsors) }}
    {% endfor %}
  </div>
{% endblock %}
